<div class="mix y{{anio}} thumbnail slider-item slider-item--{{tipo}}{{#if produccion}} slider-item--{{produccion}}{{/if}}" data-layer="{{nid}}" data-myorder="{{@index}}">
  <a class="imagen" href="#">
  	<div class="gradient-image">
  		<span class="gradient"></span>
    	<img src="{{imagen}}" />
    </div>
    <div class="caption">
      <div class="titulo">{{titulo}}</div>
      <div class="fechas">{{fechas}}</div>
    </div>
    <span class="overlay"></span>
  </a>
</div>
